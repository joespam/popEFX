.indexPage
	- @users.each_with_index do |u|
		.indexTile
			- p = Profile.find_by(user_id: u.id)
			- avatar = p.avatar.url(:medium)
			.row.center
				= link_to u do
					.profileAvatar{:style => %(background:url("#{p.avatar.url(:medium)}"); background-size: contain; background-position: 50%; background-repeat: no-repeat)}
			.row.center
				%p.profileBrandLevel
					= link_to p.brandname, u
				.lvl.lvlc4.profileBrandLevel
					= p.level